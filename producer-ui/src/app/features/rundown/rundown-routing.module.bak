import { RouterModule, Routes } from '@angular/router';
import { RundownEditorComponent } from './rundown-editor/rundown-editor.component';

const routes: Routes = [{ path: '', component: RundownEditorComponent }];

@NgModule({
  imports: [RouterModule.forChild(routes)],
